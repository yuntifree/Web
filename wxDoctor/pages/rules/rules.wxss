.rule-title {
  display: block;
  width: 100%;
  height: 48rpx;
  margin: 24rpx 0rpx;
  padding-left: 60rpx;
  font-size: 34rpx;
  text-align: left;
  font-weight: 500;
}
.colorac {
  color: #4db6ac;
}
.rule-list  {
  width: 100%;
  height: auto;
  padding: 0 60rpx;
}
.rule-list-tit {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 34rpx;
  height: 48rpx;
  line-height: 48rpx;
  text-align: left;
  font-weight: 500;
  margin-bottom: 48rpx;
  position: relative;
}
.margin20 {
  margin-bottom: 20rpx;
}
.rule-list-text {
  display: block;
  line-height: 150%;
  font-size: 32rpx;
  margin-bottom: 24rpx;
  text-align: justify;
}
.section {
  width: 100%;
  height: auto;
  padding: 0 60rpx 80rpx 60rpx;
}
.section-area {
  width: 100%;
  height: 240rpx;
  padding: 20rpx 24rpx;
  line-height: 150%;
  border-radius: 8rpx;
  background-color: #ffffff;
  border: solid 2rpx #969696;
  font-size: 32rpx;
}
.rule-ico {
  display: block;
  width: 36rpx;
  height: 36rpx;
  margin-left: -40rpx;
}
.rule-btn {
  width: 522rpx;
  margin-top: 54rpx;
}

